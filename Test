_local autoSpinToggleObj = Tabs.Fruit:AddToggle("AutoSpinToggle", {
    Title = "Auto Spin Fruit",
    Default = false,
    Callback = function(state)
        _G.autoSpinToggle = state

        if state then
            print("üåÄ B·∫≠t Auto Spin - T·∫£i script t·ª´ GitHub...")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/nhansamtinam/KimSoiHub/refs/heads/main/Auto%20Spin%20For%20Main%20Script"))()
        else
            print("üõë T·∫Øt Auto Spin")
            if _G.AutoSpinThread then
                task.cancel(_G.AutoSpinThread)
                _G.AutoSpinThread = nil
            end
        end
    end
})
-- ‚úÖ L∆∞u ƒë√∫ng toggle object
_G.AutoSpinToggleUI = autoSpinToggleObj

