--Teleport NPC
local selectedNPCName = "Soru Quest"

-- Dropdown Chá»n NPC
Tabs.Sever:AddDropdown("NPCSelector", {
    Title = "Chá»n NPC",
    Values = { "Soru Book", "Soru Quest" },
