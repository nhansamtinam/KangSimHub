    Title = "Teleport Äáº¿n Map",
    Description = "Dá»‹ch chuyá»ƒn Ä‘áº¿n map Ä‘Ã£ chá»n",
    Callback = function()
        local TeleportService = game:GetService("TeleportService")
        local Players = game:GetService("Players")
        local player = Players.LocalPlayer

        local placeId = mapIds[selectedMapName]
        if not placeId then
            warn("âŒ KhÃ´ng tÃ¬m tháº¥y ID cho map: " .. tostring(selectedMapName))
            return
        end

        print("ðŸ”¹ Äang dá»‹ch chuyá»ƒn Ä‘áº¿n:", selectedMapName, " (PlaceId:", placeId, ")")
        TeleportService:Teleport(placeId, player)
    end
})
