-- C·∫≠p nh·∫≠t: L∆∞u config ch√≠nh x√°c
local currentConfigName = ""
Tabs.Setting:AddInput("ConfigNameInput", {
    Title = "T√™n Config",
    Default = "",
    Placeholder = "Nh·∫≠p t√™n config ƒë·ªÉ l∆∞u",
    Numeric = false,
    Callback = function(value)
        currentConfigName = value
    end
})

-- Dropdown ch·ªçn config ƒë·ªÉ load
local configDropdown = Tabs.Setting:AddDropdown("ConfigSelector", {
    Title = "Ch·ªçn Config Load",
    Values = {},
    Multi = false,
    Default = "",
    Callback = function(value)
        if value and value ~= "" then
            SaveManager:LoadConfig(value)
            game.StarterGui:SetCore("SendNotification", {
                Title = "ƒê√£ Load Config",
                Text = "ƒê√£ load config: " .. value,
                Duration = 5,
                Icon = "rbxassetid://138706932256796"
            })
        end
    end
})

-- H√†m c·∫≠p nh·∫≠t danh s√°ch config
local function updateConfigListDropdown()
    local configs = SaveManager:GetConfigs() or {}
    if #configs == 0 then
        configs = { "Ch∆∞a c√≥ config n√†o" }
    end
    configDropdown:SetValues(configs)
    configDropdown:SetValue(configs[1])
end

-- N√∫t l∆∞u config
Tabs.Setting:AddButton({
    Title = "L∆∞u Config",
    Description = "L∆∞u c·∫•u h√¨nh hi·ªán t·∫°i v·ªõi t√™n ƒë√£ nh·∫≠p",
    Callback = function()
        if currentConfigName == "" then
            game.StarterGui:SetCore("SendNotification", {
                Title = "L·ªói",
                Text = "Vui l√≤ng nh·∫≠p t√™n config tr∆∞·ªõc khi l∆∞u!",
                Duration = 5,
                Icon = "rbxassetid://138706932256796"
            })
            return
        end
        SaveManager:SaveConfig(currentConfigName)
        SaveManager:SetAutoloadConfig(currentConfigName)

        game.StarterGui:SetCore("SendNotification", {
            Title = "Th√†nh C√¥ng",
            Text = "ƒê√£ l∆∞u config: " .. currentConfigName,
            Duration = 5,
            Icon = "rbxassetid://138706932256796"
        })
        updateConfigListDropdown()
    end
})

-- G·ªçi c·∫≠p nh·∫≠t config l√∫c kh·ªüi t·∫°o
updateConfigListDropdown()
-- T·ª± ƒë·ªông load config ƒë√£ ƒë∆∞·ª£c ƒë√°nh d·∫•u l√† autoload (n·∫øu c√≥)
local autoloadConfig = SaveManager:GetAutoloadConfig()
if autoloadConfig and autoloadConfig ~= "" then
    SaveManager:LoadConfig(autoloadConfig)
    game.StarterGui:SetCore("SendNotification", {
        Title = "Autoload",
        Text = "ƒê√£ t·ª± ƒë·ªông load config: " .. autoloadConfig,
        Duration = 5,
        Icon = "rbxassetid://138706932256796"
    })
end
Tabs.Setting:AddParagraph("AutoloadInfo", "Config t·ª± ƒë·ªông load hi·ªán t·∫°i: " .. (autoloadConfig or "Kh√¥ng c√≥"))
