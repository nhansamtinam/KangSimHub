-- Tải và chạy script 1: AutoCheckManaRieng
local success1, err1 = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/nhansamtinam/KimSoiHub/refs/heads/main/AutoCheckManaRieng"))()
end)

if not success1 then
    warn("Lỗi khi chạy script AutoCheckManaRieng: " .. tostring(err1))
end

-- Đợi một chút trước khi chạy tiếp (nếu cần)
task.wait(1)

